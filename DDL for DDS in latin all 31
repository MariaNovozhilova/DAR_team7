CREATE TABLE IF NOT EXISTS dds.dbms_and_employee_level
( id integer primary key,
 "dbms" varchar (50),
 "updated_at" TIMESTAMP WITH TIME ZONE,
 "sort" integer,
 "grade" varchar (50),
 "active" bool,
 "date" date,
 "user_id" integer);

CREATE TABLE IF NOT EXISTS dds.dbms 
(id integer primary key,
 "updated_at" TIMESTAMP WITH TIME ZONE,
 "sort" integer,
 "active" bool,
 "dbms" text);

CREATE TABLE IF NOT EXISTS dds.program_and_employee_level
(id integer primary key,
 "updated_at" TIMESTAMP WITH TIME ZONE,
 "sort" integer,
 "grade" varchar (50),
 "active" bool,
 "date" date,
 "program" varchar (50),
 "user_id" integer);

CREATE TABLE IF NOT EXISTS dds.program 
(id integer primary key,
 "updated_at" TIMESTAMP WITH TIME ZONE,
 "sort" integer,
 "active" bool,
 "program" text);

CREATE TABLE IF NOT EXISTS dds.education 
("user_id" integer,
 id integer primary key,
 "year_graduated" integer,
 "updated_at" TIMESTAMP WITH TIME ZONE,
 "institution_name" text,
 "sort" integer,
 "education_level" varchar (50),
 "faculty_department" text,
 "short_name" text,
 "active" bool,
 "qualification" text,
 "specialty" text);

CREATE TABLE IF NOT EXISTS dds.industry_employee_experience 
("user_id" integer,
 id integer primary key,
